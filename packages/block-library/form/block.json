{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "omniform/form",
	"category": "omniform",
	"title": "OmniForm",
	"description": "Easily create professional forms for your website",
	"textdomain": "omniform",
	"attributes": {
		"ref": {
			"type": "number"
		}
	},
	"supports": {
		"anchor": true,
		"align": [ "full", "wide" ],
		"html": false,
		"reusable": false,
		"color": {
			"background": true,
			"text": true,
			"link": true,
			"gradients": true
		},
		"spacing": {
			"margin": [ "top", "bottom" ],
			"padding": true,
			"blockGap": true,
			"__experimentalDefaultControls": {
				"blockGap": true
			}
		},
		"__experimentalBorder": true,
		"__experimentalLayout": {
			"allowEditing": false
		}
	},
	"usesContext": [
		"postId",
		"postType",
		"omniform/fieldGroupName"
	],
	"styles": [
		{ "name": "default", "label": "Default", "isDefault": true },
		{ "name": "inverted", "label": "Inverted" }
	],
	"editorScript": "file:index.js",
	"editorStyle": "file:index.css",
	"style": "file:style-index.css",
	"viewScript": "file:view.js"
}