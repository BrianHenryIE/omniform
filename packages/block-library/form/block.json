{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "omniform/form",
	"category": "form",
	"title": "OmniForm",
	"textdomain": "omniform",
	"usesContext": [
		"postId",
		"postType"
	],
	"attributes": {
		"ref": {
			"type": "number"
		}
	},
	"supports": {
		"align": true,
		"html": false,
		"reusable": false,
		"color": {
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		}
	},
	"editorScript": "file:index.js",
	"editorStyle": "file:index.css",
	"style": "file:style-index.css",
	"viewScript": "file:view.js"
}