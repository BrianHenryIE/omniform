{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "omniform/field-select",
	"category": "form-control",
	"title": "Select",
	"description": "A field with multiple options where a single choice can be made.",
	"textdomain": "omniform",
	"attributes": {
		"multiple": {
			"type": "boolean",
			"default": false
		},
		"placeholder": {
			"type": "string"
		},
		"label": {
			"type": "string"
		},
		"help": {
			"type": "string"
		}
	},
	"supports": {
		"html": false
	},
	"editorScript": "file:index.js",
	"editorStyle": "file:index.css",
	"style": "file:style-index.css"
}