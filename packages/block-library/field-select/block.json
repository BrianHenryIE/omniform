{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "omniform/field-select",
	"category": "form-control",
	"title": "Select",
	"description": "A field with multiple options where a single choice can be made.",
	"textdomain": "omniform",
	"attributes": {
		"isMultiple": {
			"type": "boolean",
			"default": false
		},
		"fieldType": {
			"type": "string",
			"default": "text"
		},
		"fieldPlaceholder": {
			"type": "string"
		},
		"fieldLabel": {
			"type": "string"
		},
		"fieldName": {
			"type": "string"
		},
		"fieldValue": {
			"type": "string"
		},
		"height": {
			"type": "integer"
		},
		"isRequired": {
			"type": "boolean"
		}
	},
	"providesContext": {
		"omniform/fieldSelectName": "fieldName"
	},
	"supports": {
		"align": true,
		"html": false,
		"color": {
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		}
	},
	"usesContext": [ "omniform/fieldGroupName" ],
	"editorScript": "file:index.js",
	"editorStyle": "file:index.css",
	"style": "file:style-index.css"
}