{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "omniform/button",
	"category": "omniform-control-simple",
	"title": "Button",
	"description": "A button for the form",
	"textdomain": "omniform",
	"attributes": {
		"buttonType": {
			"type": "string",
			"default": "button"
		},
		"buttonLabel": {
			"type": "string"
		},
		"buttonName": {
			"type": "string"
		},
		"buttonValue": {
			"type": "string"
		},
		"backgroundColor": {
			"type": "string"
		},
		"textColor": {
			"type": "string"
		}
	},
	"supports": {
		"align": true,
		"html": false,
		"color": {
			"gradients": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		},
		"__experimentalBorder": true,
		"__experimentalSelector": ".wp-block-omniform-button .wp-element-button"
	},
	"styles": [
		{ "name": "fill", "label": "Fill", "isDefault": true },
		{ "name": "outline", "label": "Outline" }
	],
	"editorScript": "file:index.js",
	"editorStyle": "file:index.css",
	"style": "file:style-index.css"
}