{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "omniform/field-input",
	"category": "form-control",
	"title": "Text",
	"description": "A text box for short responses.",
	"textdomain": "omniform",
	"attributes": {
		"fieldType": {
			"type": "string",
			"default": "text"
		},
		"fieldPlaceholder": {
			"type": "string"
		},
		"fieldLabel": {
			"type": "string"
		},
		"fieldName": {
			"type": "string"
		},
		"fieldValue": {
			"type": "string"
		}
	},
	"supports": {
		"align": true,
		"html": false
	},
	"usesContext": [ "omniform/fieldGroupName" ],
	"editorScript": "file:index.js",
	"editorStyle": "file:index.css",
	"style": "file:style-index.css"
}