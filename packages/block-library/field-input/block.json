{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "omniform/field-input",
	"category": "form-control",
	"title": "Text",
	"description": "A text box for short responses.",
	"textdomain": "omniform",
	"attributes": {
		"type": {
			"type": "string",
			"default": "text"
		},
		"placeholder": {
			"type": "string"
		},
		"label": {
			"type": "string"
		},
		"name": {
			"type": "string"
		},
		"value": {
			"type": "string"
		},
		"isHoneypot": {
			"type": "boolean"
		}
	},
	"supports": {
		"align": true,
		"html": false
	},
	"editorScript": "file:index.js",
	"editorStyle": "file:index.css",
	"style": "file:style-index.css"
}